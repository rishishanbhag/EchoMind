{
  "version": 2,
  "builds": [
    {
      "src": "client/geminiapp/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/geminiapp/dist/$1"
    }
  ],
  "outputDirectory": "client/geminiapp/dist"
}