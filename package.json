{
  "name": "echomind",
  "version": "1.0.0",
  "description": "AI-powered chat application with image support",
  "scripts": {
    "dev:api": "cd api && npm run dev",
    "dev:client": "cd client/geminiapp && npm run dev",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "build": "cd client/geminiapp && npm run build",
    "start": "cd api && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
